<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.HBox?>
<?import com.jfoenix.controls.JFXButton?>
<HBox xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="grader.Controller"
      stylesheets="/grader/sidebarTitledPane.css">
    <TitledPane fx:id="titledPane" HBox.hgrow="ALWAYS" maxWidth="Infinity">
        <AnchorPane>
            <VBox fx:id="contentWrapper" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <JFXButton fx:id="addButton" styleClass="raisedBtn">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </JFXButton>
            </VBox>
        </AnchorPane>
    </TitledPane>
</HBox>